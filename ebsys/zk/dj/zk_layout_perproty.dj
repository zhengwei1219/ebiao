ml.documentElement.selectSingleNode("/record/viewtype").text);&#13;&#10;	}&#13;&#10;	uf_showViewHelp();//显示视图类型&#13;&#10;	var toolbarPermit = fcpubdata.obj[1].getAttribute("toolbarPermit")&#13;&#10;	if(IsSpace(toolbarPermit) == false){&#13;&#10;		var dsDom = SetDom("<root>"+unescape(toolbarPermit)+"</root>");&#13;&#10;		var trData = "";&#13;&#10;		for(var j=0;j<dsDom.documentElement.childNodes.length;j++){&#13;&#10;			var oDom = SetDom(dsDom.documentElement.childNodes[j].xml);&#13;&#10;			trData += "<tr><td>"+oDom.documentElement.getAttribute("id")+"</td><td>"+oDom.documentElement.getAttribute('controltype')+"</td><td>"+RepXml(dsDom.documentElement.childNodes[j].xml)+"</td></tr>";&#13;&#10;		}&#13;&#10;		var dsXml2 = dataset2.xml;&#13;&#10;		dsXml2 = "<root>"+ trData + dsXml2.substring(dsXml2.indexOf("<set><pages>"),dsXml2.length);&#13;&#10;		dataset2.OpenXmlData(dsXml2);&#13;&#10;	}&#13;&#10;}&#13;&#10;&#13;&#10;function uf_ok(){&#13;&#10;	&#13;&#10;	if(txtDjsn.value == ""){&#13;&#10;		alert("模板名不能为空");&#13;&#10;		return;&#13;&#10;	}&#13;&#10;	&#13;&#10;	if(txtChnName.value == ""){&#13;&#10;		txtChnName.value = txtDjsn.value;&#13;&#10;	}&#13;&#10;	if(div1.style.display == "block"){&#13;&#10;		var ret = IsInt($id('txtShowHeight').value);&#13;&#10;		if(ret == false){&#13;&#10;			alert('显示高度不是有效的整数');	&#13;&#10;			return;&#13;&#10;		}&#13;&#10;		&#13;&#10;	}else{&#13;&#10;		SetCheckBoxValue($id("isPrint"), '0');&#13;&#10;	}&#13;&#10;	var recordPermit = "";&#13;&#10;	var len = dataset2.oDom.documentElement.childNodes.length-1;	                   &#13;&#10;	if(len > 0){&#13;&#10;		// dsRecordData = escape(dataset2.RecordData); &#13;&#10;		for(var i=0;i<len;i++){&#13;&#10;			recordPermit += dataset2.oDom.documentElement.childNodes[i].childNodes[2].text;	&#13;&#10;		}&#13;&#10;		recordPermit = escape(recordPermit);&#13;&#10;	}&#13;&#10;	fcpubdata.obj[0].oXml.documentElement.selectSingleNode("/record/name").text = $id("txtDjsn").value;&#13;&#10;	fcpubdata.obj[0].name = $id("txtDjsn").value;&#13;&#10;	fcpubdata.obj[0].oXml.documentElement.selectSingleNode("/record/chnname").text = $id("txtChnName").value;&#13;&#10;	fcpubdata.obj[0].oXml.documentElement.selectSingleNode("/record/isprintfile").text = $id("isPrint").value;&#13;&#10;	fcpubdata.obj[0].oXml.documentElement.selectSingleNode("/record/isrunebiao").text = $id("comRunEbiao").value;&#13;&#10;	fcpubdata.obj[0].oXml.documentElement.selectSingleNode("/record/classtable").text = $id('txtClassName').value;&#13;&#10;	fcpubdata.obj[0].oXml.documentElement.selectSingleNode("/record/styletable").text = $id('txtStyle').value;&#13;&#10;	fcpubdata.obj[0].oXml.documentElement.selectSingleNode("/record/showheight").text = $id('txtShowHeight').value;&#13;&#10;	fcpubdata.obj[0].oXml.documentElement.selectSingleNode("/record/isfixrowcol").text = $id('isFixRowCol').value;&#13;&#10;	fcpubdata.obj[0].oXml.documentElement.selectSingleNode("/record/viewtype").text = $id('viewType').value;&#13;&#10;	fcpubdata.obj[0].isSaveFile = $id("isSaveFile").value;&#13;&#10;	fcpubdata.obj[1].setAttribute("toolbarPermit",recordPermit);&#13;&#10;	$eform('关闭窗口');&#13;&#10;}&#13;&#10;//页签控件的点击事件&#13;&#10;function PubClickPageControl(index) {&#13;&#10;	if(index == 1){&#13;&#10;		grid1.ParentPos = "相对" ;&#13;&#10;		grid1.hideVscroll = false ;&#13;&#10;		grid1.hideHscroll = false ;&#13;&#10;		grid1.HScroll();  //重算滚动条&#13;&#10;		grid1.VScroll();&#13;&#10;	}&#13;&#10;}&#13;&#10;function uf_getStyle(){&#13;&#10;	 var ret = DjOpen('../../fceform/common/djframe.htm?djsn=zk_style_prop&djtype=ZK&type=1',[$id('txtClassName'). value,$id('txtStyle').value]); &#13;&#10;	 if(IsSpace(ret) == false){&#13;&#10;	 	$id('txtClassName').value = ret[0];&#13;&#10;	 	$id('txtStyle').value = ret[1];	&#13;&#10;	 }	&#13;&#10;}&#13;&#10;&#13;&#10;&#13;&#10;function uf_hide(){&#13;&#10;	var display = "block";&#13;&#10;	var isPrint = 1;&#13;&#10;	var isFixRow = 1;&#13;&#10;	if(comRunEbiao.value =="3"){&#13;&#10;		display = "none";&#13;&#10;		isPrint = 0;&#13;&#10;		isFixRow = 0;&#13;&#10;	}else if(comRunEbiao.value =="2"){&#13;&#10;		display = "none";&#13;&#10;		isPrint = 0;&#13;&#10;		isFixRow = 1;&#13;&#10;	}&#13;&#10;	SetCheckBoxValue($id("isPrint"), isPrint);&#13;&#10;	SetCheckBoxValue($id("isFixRowCol"), isFixRow);&#13;&#10;	div1.style.display = display;	&#13;&#10;}&#13;&#10;&#13;&#10;function uf_fx(fieldName){&#13;&#10;	DjOpen("../../fceform/common/djframe.htm?djsn=toolbarPermit&djtype=LR",[dataset2,fieldName],"展现","有模式窗口","直接","选择");	&#13;&#10;&#13;&#10;}&#13;&#10;&#13;&#10;function uf_getPermit(){&#13;&#10;	var toolbarPermit = t.toolbarPermit;//e表设计器table中的toolbarPermit属性&#13;&#10;	var id="replace_Control_Id_NamePcm";//替身控件的id名,怕重名（pcm）&#13;&#10;	if(IsSpace(dataset2.Field('permitSet').Value) == false) {&#13;&#10;		var permitSet = dataset2.Field('permitSet').Value;&#13;&#10;		permitSet = permitSet.replace('id="'+ dataset2.Field('contId').Value+'"','id="'+ id + '"');//把contId字段的值变成替身控件的id&#13;&#10;		$id('lblTitle').roleXml = permitSet;&#13;&#10;	}else{&#13;&#10;		$id('lblTitle').roleXml = "";&#13;&#10;	}&#13;&#10;	if(IsSpace(dataset2.Field('contId').Value) == true){&#13;&#10;		alert("先输入控件id");&#13;&#10;		return;	&#13;&#10;	}&#13;&#10;	$id(id).controltype = dataset2.Field('contType').Value;&#13;&#10;	var param1 = fcpubdata.obj[0];&#13;&#10;	var param2 = fcpubdata.obj[1];&#13;&#10;    var arr = [$id(id),'', $id('lblTitle')];&#13;&#10;    fcpubdata.obj = arr;&#13;&#10;    EformActionButtonClick();&#13;&#10;    fcpubdata.obj[2].roleXml = fcpubdata.obj[2].roleXml.replace("id='"+ id + "'", "id='"+ dataset2.Field('contId').Value+"'");//替身控件id变contId字段的值&#13;&#10;    dataset2.Field('permitSet').Value = fcpubdata.obj[2].roleXml;&#13;&#10;	var ret = dataset2.Update();&#13;&#10;	if(!IsSpace(ret)){&#13;&#10;		alert(ret);&#13;&#10;		return;	&#13;&#10;	}&#13;&#10;	dataset2.fset_cont();&#13;&#10;	grid1.hide();&#13;&#10;	fcpubdata.obj[0] = param1;&#13;&#10;	fcpubdata.obj[1] = param2;&#13;&#10;}&#13;&#10;&#13;&#10;&#13;&#10;function uf_selContTypeAfter(){//选择改变控件类型后，修改控件权限字段的控件类型&#13;&#10;	if(IsSpace(dataset2.Field('permitSet').Value) == false) {&#13;&#10;		var oXml = SetDom(dataset2.Field('permitSet').Value);&#13;&#10;		//alert(dataset2.Field('contType').Value);&#13;&#10;		oXml.documentElement.setAttribute('controltype',dataset2.Field('contType').Value);&#13;&#10;		&#13;&#10;		dataset2.Field('permitSet').Value = oXml.xml;&#13;&#10;		var ret = dataset2.Update();&#13;&#10;		if(!IsSpace(ret)){&#13;&#10;			alert(ret);&#13;&#10;			return;	&#13;&#10;		}&#13;&#10;		dataset2.fset_cont();&#13;&#10;	}&#13;&#10;}&#13;&#10;&#13;&#10;function uf_showViewHelp(){&#13;&#10;	switch ($id('viewType').value)&#13;&#10;    {&#13;&#10;    	case "列表式" : $id('help').innerHTML="  列表式：是一个二维表显示，第一行为标题行，下面是记录行。在《选择表》界面中，查询信息类型要选择电脑或手机上用，明细数据集时一条记录所占的行数必须大于0。列表式属于多记录，所以修改和删除时需要知道行记录的id，所以要用无线按钮控件绑定id字段，或在行上设置删除或修改的button按钮设置id字段，或在行的开始单元格上直接添加一个id字段。";break;&#13;&#10;    	case "编辑式" :$id('help').innerHTML="编辑式：只一条记录，记录中的字段不像列表式受行列的限制，在《选择表》界面中，查询信息类型要选择电脑或手机上用，选择明细数据集所占的行数为0";break;&#13;&#10;    	case "查询式" : $id('help').innerHTML="查询式：在《选择表》界面中，选择的查询信息的类型一定要是电脑或手机上查询信息时用。";break;&#13;&#10;    	case "交叉表" :$id('help').innerHTML="交叉表：在布局模板设计器的e表设计器中，设置交叉报表。";break;&#13;&#10;		case "主从表" : $id('help').innerHTML="主从表：主表是卡片式，从表是列表式。";break;&#13;&#10;    	case " " :$id('help').innerHTML="";break;&#13;&#10;&#13;&#10;	}&#13;&#10;}&#13;&#10;</script> 
