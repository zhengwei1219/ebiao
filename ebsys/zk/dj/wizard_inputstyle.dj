<script>var objNav=new Object();&#13;&#10;objNav.prevPage = "wizard_maininput" ;&#13;&#10;objNav.nextPage = "wizard_subinput" ;&#13;&#10;objNav.saveConfig = saveConfig;&#13;&#10;objNav.isOk = true;&#13;&#10;function uf_open() {&#13;&#10;	objNav.config = parent.parent.toolbar.WizardConfig ;&#13;&#10;	if(objNav.config.left!=null) txtLeft.value=objNav.config.left;&#13;&#10;	if(objNav.config.top!=null) txtTop.value=objNav.config.top;&#13;&#10;	if(objNav.config.rightbottom!=null) textRightbottom.value=objNav.config.rightbottom;&#13;&#10;	if(objNav.config.adjust!=null) txtAdjust.value=objNav.config.adjust;&#13;&#10;	if(objNav.config.bigWidth!=null) txtBigWidth.value=objNav.config.bigWidth;&#13;&#10;	if(objNav.config.labelHeight!=null) txtLabelHeight.value=objNav.config.labelHeight;	&#13;&#10;	if(IsSpace(objNav.config.inputStyle))&#13;&#10;		SetRadioValue($id("rdoInputStyle"),"3");&#13;&#10;	else&#13;&#10;		SetRadioValue($id("rdoInputStyle"),objNav.config.inputStyle);&#13;&#10;	if(IsSpace(objNav.config.inputCols) == false)&#13;&#10;		$id("txtCols").value = objNav.config.inputCols;&#13;&#10;    SetCheckBoxValue($id("chkHaveSub"),objNav.config.isHaveSub);	&#13;&#10;	uf_sel();&#13;&#10;	uf_nav();&#13;&#10;	parent.parent.toolbar.execScript("new Eform.Wizard().disabledButton();")&#13;&#10;}&#13;&#10;//保存配置信息,由工具栏来调用&#13;&#10;function saveConfig(){&#13;&#10;	objNav.config.inputStyle = $id("rdoInputStyle").value;&#13;&#10;	objNav.config.inputCols = $id("txtCols").value;&#13;&#10;	objNav.config.isHaveSub = $id("chkHaveSub").value;&#13;&#10;	objNav.config.left = $id("txtLeft").value;&#13;&#10;	objNav.config.top = $id("txtTop").value;&#13;&#10;	objNav.config.rightbottom = $id("textRightbottom").value;&#13;&#10;	objNav.config.adjust = $id("txtAdjust").value;&#13;&#10;	objNav.config.bigWidth = $id("txtBigWidth").value;&#13;&#10;	objNav.config.labelHeight = $id("txtLabelHeight").value;&#13;&#10;	objNav.isOk = true;&#13;&#10;}&#13;&#10;function uf_sel(){&#13;&#10;	var inStyle = $id("rdoInputStyle").value;&#13;&#10;	if(inStyle == "1" || inStyle == "2" || inStyle == "7"){&#13;&#10;		$id("lblCols").style.display="none";&#13;&#10;		$id("txtCols").style.display="none";&#13;&#10;		$id("chkHaveSub").style.display="none";&#13;&#10;		SetCheckBoxValue($id("chkHaveSub"),"否");&#13;&#10;	}else{&#13;&#10;		$id("lblCols").style.display="";&#13;&#10;		$id("txtCols").style.display="";&#13;&#10;		$id("chkHaveSub").style.display="";&#13;&#10;	}&#13;&#10;	//uf_selList();&#13;&#10;	if(inStyle == "3" || inStyle == "4" || inStyle == "8"){&#13;&#10;	 	$id("divStyle").style.display="";&#13;&#10;	 	$id("divLabel").style.display="";&#13;&#10;	}else{&#13;&#10;		$id("divStyle").style.display="none";&#13;&#10;		$id("divLabel").style.display="none";&#13;&#10;	}&#13;&#10;	if(inStyle == "1"){&#13;&#10;		$id("divSelMode").style.display="none";&#13;&#10;	}else{&#13;&#10;		$id("divSelMode").style.display="";&#13;&#10;		if(inStyle == "2" || inStyle == "7"){&#13;&#10;			$id("imgSelMode").src = "../../fceform/images/ef_design_textgrid.jpg";&#13;&#10;		}else if(inStyle == "4" || inStyle == "6"){&#13;&#10;			$id("imgSelMode").src = "../../fceform/images/ef_design_text1.jpg" ;&#13;&#10;		}else{&#13;&#10;			$id("imgSelMode").src = "../../fceform/images/ef_design_text.jpg" ;&#13;&#10;		}&#13;&#10;	}&#13;&#10;}&#13;&#10;&#13;&#10;function uf_nav(){&#13;&#10;	if(($id("rdoInputStyle").value == "3" || $id("rdoInputStyle").value == "4" || $id("rdoInputStyle").value == "5" || $id("rdoInputStyle").value == "6" || $id("rdoInputStyle").value == "8")&#13;&#10;		 && $id("chkHaveSub").value=="是"){&#13;&#10;			objNav.nextPage = "wizard_subinput" ;&#13;&#10;	}else{&#13;&#10;		objNav.nextPage = "wizard_listTable" ;&#13;&#10;	}&#13;&#10;		&#13;&#10;	parent.parent.toolbar.execScript("new Eform.Wizard().disabledButton();")&#13;&#10;}&#13;&#10;</script> 
