<script>&#13;&#10;//打开表单时函数&#13;&#10;function open() {&#13;&#10;	PageControl1.HideTitle()//隐藏页签标题FCButton21&#13;&#10;    SetButtonImage(cmdUp,"../images/ef_run_button_up.gif") ;&#13;&#10;    SetButtonImage(cmdDown,"../images/ef_run_button_down.gif") ;&#13;&#10;    SetButtonImage(FCButton21,"../images/ef_run_button_up.gif") ;&#13;&#10;    SetButtonImage(FCButton22,"../images/ef_run_button_down.gif") ;&#13;&#10;	SetCheckBoxValue(chkAddChildTab,"否") ;&#13;&#10;	SetCheckBoxValue(chkNav,"是");&#13;&#10;	SetRadioValue(rdoSelAddTab,'2');&#13;&#10;	uf_SelAddTab();&#13;&#10;}&#13;&#10;//选择新建空白表单还是用向导建表&#13;&#10;function uf_SelAddTab() {&#13;&#10;	if(rdoSelAddTab.value == 1){&#13;&#10;		cmdBack.disabled = true;&#13;&#10;		cmdNext.disabled = true;&#13;&#10;		cmdFinish.disabled = false;&#13;&#10;	}else{&#13;&#10;		cmdBack.disabled = false ;&#13;&#10;		cmdNext.disabled = false;&#13;&#10;		if(PageControl1.selectedIndex==0){&#13;&#10;			cmdFinish.disabled = true;	&#13;&#10;			cmdBack.disabled = true;&#13;&#10;		}&#13;&#10;	}&#13;&#10;}&#13;&#10;//完成事件	SKDBRadioGroup10&#13;&#10;&#13;&#10;function Sel_Click() {&#13;&#10;	if(rdoSelAddTab.value == 1){&#13;&#10;		window.returnValue="空" ;&#13;&#10;		window.close();&#13;&#10;	}else{&#13;&#10;		uf_ok();&#13;&#10;	}&#13;&#10;}&#13;&#10;//上一页&#13;&#10;function uf_PrevPage() {&#13;&#10;	PageControl1.PrevPage();&#13;&#10;	if(rdoSelAddTab.value == 2) {&#13;&#10;		cmdFinish.disabled = true;&#13;&#10;	}&#13;&#10;	if(PageControl1.selectedIndex==0){&#13;&#10;		cmdBack.disabled = true;	&#13;&#10;	}else{&#13;&#10;		cmdFinish.disabled = false;	&#13;&#10;	}	&#13;&#10;	//走两页&#13;&#10;	if(PageControl1.selectedIndex == 3){&#13;&#10;		if(chkAddChildTab.value == "是" ) {&#13;&#10;			PageControl1.PrevPage(); //上一页&#13;&#10;		}&#13;&#10;	}&#13;&#10;	if(PageControl1.selectedIndex == 4){&#13;&#10;		if(SKDBRadioGroup9.value == 2){&#13;&#10;			PageControl1.PrevPage(); //上一页&#13;&#10;		}&#13;&#10;	}&#13;&#10;	if(PageControl1.selectedIndex != 5 ) {&#13;&#10;		cmdFinish.disabled = true;&#13;&#10;		cmdNext.disabled = false;&#13;&#10;	}else{&#13;&#10;		cmdFinish.disabled = false;&#13;&#10;		cmdNext.disabled = true;&#13;&#10;	}&#13;&#10;}&#13;&#10;&#13;&#10;//下一页&#13;&#10;function uf_NextPage() {&#13;&#10;	//页签在第二页时,主表名称,主表的主键不能为空&#13;&#10;	if(PageControl1.selectedIndex==1){&#13;&#10;		if(cboSelPkTab.value == "" || lstSelField2.options.length==0 || cboSelPkField.value == ""){&#13;&#10;			alert("请选择表名、请选择字段和主键!") ;&#13;&#10;			return ;&#13;&#10;		}&#13;&#10;		//主键在主表字段中是否存在&#13;&#10;		if(IsTrueValue() != true ){&#13;&#10;			alert("已选字段中没有主键字段,请填加主键字段或修改主键！");&#13;&#10;			return ;&#13;&#10;		}&#13;&#10;&#13;&#10;	}&#13;&#10;	if(PageControl1.selectedIndex == 2){&#13;&#10;		if(chkAddChildTab.value == "是") {&#13;&#10;			//子表的表名和子段&#13;&#10;			if(cboSelChildTabName.value == "" || lstSelFieldRight.options.length == 0){&#13;&#10;				alert("请选择子表表名,字段名称不能为空!");&#13;&#10;				return;&#13;&#10;			}&#13;&#10;			//子表和主表是否有关联字段&#13;&#10;			if(IsTabPkValue() != true) {&#13;&#10;				alert("请选择子表与主表的关联字段！");&#13;&#10;				return;&#13;&#10;			}&#13;&#10;		}&#13;&#10;	}&#13;&#10;	PageControl1.NextPage(); //下一页&#13;&#10;	cmdFinish.disabled = false;&#13;&#10;	&#13;&#10;	//页签不等于第一页,下一步按钮为活动&#13;&#10;	if(PageControl1.selectedIndex!=0){&#13;&#10;		cmdBack.disabled = false;	&#13;&#10;	}&#13;&#10;	if(PageControl1.selectedIndex == 3){&#13;&#10;		if(chkAddChildTab.value == "是" ) {&#13;&#10;			PageControl1.NextPage(); //下一页&#13;&#10;			&#13;&#10;		}&#13;&#10;			SetRadioValue(SKDBRadioGroup9,'1');&#13;&#10;		&#13;&#10;	}&#13;&#10;	if(PageControl1.selectedIndex == 4){&#13;&#10;		if(SKDBRadioGroup9.value == 2  || SKDBRadioGroup9.value == 3){&#13;&#10;			PageControl1.NextPage(); //下一页&#13;&#10;		}else{&#13;&#10;			SKDBRadioGroup10.value = 1 ;//SetRadioValue(SKDBRadioGroup10,"1");&#13;&#10;		}&#13;&#10;	}&#13;&#10;	&#13;&#10;	if(PageControl1.selectedIndex == 5){&#13;&#10;		rdoCmdStyle.value = 1 ;&#13;&#10;		//SetRadioValue(rdoCmdStyle,'1');&#13;&#10;		uf_Selcmd();&#13;&#10;	}&#13;&#10;	//下一页按钮和完成按钮是否活动&#13;&#10;	if(PageControl1.selectedIndex != 5 ) {&#13;&#10;		cmdFinish.disabled = true;&#13;&#10;		cmdNext.disabled = false;&#13;&#10;	}else{&#13;&#10;		cmdFinish.disabled = false;&#13;&#10;		cmdNext.disabled = true;&#13;&#10;	}&#13;&#10;	uf_Selgrid();&#13;&#10;	uf_SelPhoto();&#13;&#10;	chk_Ischeck();&#13;&#10;	&#13;&#10;}&#13;&#10;//选择主表名称,在lstSelField2控件中列出选择主表中的字段&#13;&#10;function uf_SelTabValue() {&#13;&#10;	lstSelField1.options.length = 0;&#13;&#10;	lstSelField2.options.length = 0;&#13;&#10;	var curTab = cboSelPkTab.value;&#13;&#10;	cboSelPkField.sql1 = "select FC_FLDLIST.fdname,FC_FLDLIST.chnname,FC_TBSTRU.fdname,FC_TBSTRU.tbname from FC_FLDLIST,FC_TBSTRU where rtrim(FC_TBSTRU.tbname)='"+curTab+"' and FC_TBSTRU.fdname=FC_FLDLIST.fdname" ;&#13;&#10;	var sql = "select FC_FLDLIST.fdname,FC_FLDLIST.chnname from FC_FLDLIST,FC_TBSTRU where rtrim(FC_TBSTRU.tbname)='"+curTab+"' and FC_TBSTRU.fdname=FC_FLDLIST.fdname" ;&#13;&#10;	&#13;&#10;	SqlCombo(lstSelField2,sql)&#13;&#10;	&#13;&#10;}&#13;&#10;//选择子表名称,在lstSelFieldRight控件中列出选择子表中的字段&#13;&#10;function uf_SelChildTab() {&#13;&#10;	lstSelFieldLeft.options.length = 0 ;&#13;&#10;	lstSelFieldRight.options.length = 0;&#13;&#10;	var Sel_curchildTab = cboSelChildTabName.value;&#13;&#10;	var sql = "select FC_FLDLIST.fdname,FC_FLDLIST.chnname from FC_FLDLIST,FC_TBSTRU where rtrim(FC_TBSTRU.tbname)='"+Sel_curchildTab+"' and FC_TBSTRU.fdname=FC_FLDLIST.fdname" ;&#13;&#10;	SqlCombo(lstSelFieldRight,sql);&#13;&#10;}&#13;&#10;//objLeft向右objRight增加值,&#13;&#10;function add(objLeft,objRight,sValue){	&#13;&#10;	for (var i=objLeft.length-1;i>=0;i--){&#13;&#10;		if (objLeft.options(i).selected){&#13;&#10;			var oOpt=document.createElement("option")&#13;&#10;			oOpt.text=objLeft.options(i).text&#13;&#10;			oOpt.value=objLeft.options(i).value&#13;&#10;			if (objLeft.id==sValue){&#13;&#10;				objRight.add(oOpt)&#13;&#10;			}&#13;&#10;			&#13;&#10;			objLeft.remove(i)&#13;&#10;		&#13;&#10;		}&#13;&#10;	}&#13;&#10;}&#13;&#10;//把objLeft中所有的值给objRight控件&#13;&#10;function uf_addlist(objLeft,objRight,sValue) {	&#13;&#10;	for (var i=objLeft.length-1;i>=0;i--){&#13;&#10;		var oOpt=document.createElement("option")&#13;&#10;		oOpt.text=objLeft.options(i).text&#13;&#10;		oOpt.value=objLeft.options(i).value&#13;&#10;		if (objLeft.id==sValue){&#13;&#10;			objRight.add(oOpt)&#13;&#10;		}&#13;&#10;		objLeft.remove(i)&#13;&#10;	}&#13;&#10;}&#13;&#10;//objRight向右objLeft增加值,&#13;&#10;function Sellist(objLeft,objRight,sValue) {&#13;&#10;	for (var i=objRight.length-1;i>=0;i--){&#13;&#10;		if (objRight.options(i).selected){&#13;&#10;			var oOpt=document.createElement("option")&#13;&#10;			oOpt.text=objRight.options(i).text&#13;&#10;			oOpt.value=objRight.options(i).value&#13;&#10;			if (objRight.id == sValue){&#13;&#10;				objLeft.add(oOpt)&#13;&#10;			}&#13;&#10;			objRight.remove(i)&#13;&#10;		}&#13;&#10;	}	&#13;&#10;}&#13;&#10;//把objRight中所有的值给objLeft控件&#13;&#10;function uf_del(objLeft,objRight,sValue) {&#13;&#10;	for (var i=objRight.length-1;i>=0;i--){&#13;&#10;		var oOpt=document.createElement("option")&#13;&#10;		oOpt.text=objRight.options(i).text&#13;&#10;		oOpt.value=objRight.options(i).value&#13;&#10;		if (objRight.id==sValue){&#13;&#10;			objLeft.add(oOpt)&#13;&#10;		}&#13;&#10;&#13;&#10;		objRight.remove(i)&#13;&#10;	}&#13;&#10;		&#13;&#10;}&#13;&#10;//listbox控件的值上移&#13;&#10;function uf_up(obj){&#13;&#10;     for (var i=obj.length-1;i>0;i--){&#13;&#10;      if (obj.options(i).selected == true){&#13;&#10;        var s&#13;&#10;        var d&#13;&#10;        s=obj.options(i-1).text ;&#13;&#10;        d=obj.options(i-1).value ;&#13;&#10;        obj.options(i-1).text=obj.options(i).text ;&#13;&#10;        obj.options(i-1).value=obj.options(i).value ;&#13;&#10;        obj.options(i).text=s ;&#13;&#10;        obj.options(i).value=d ;&#13;&#10;        obj.options(i).selected=false ;&#13;&#10;        obj.options(i-1).selected=true ;&#13;&#10;        break ;&#13;&#10;      }&#13;&#10;    }&#13;&#10;}&#13;&#10;//listbox控件的值下移&#13;&#10;function uf_down(obj){&#13;&#10;   for (var i=0;i<obj.length-1;i++){&#13;&#10;    if (obj.options(i).selected == true){&#13;&#10;      var s="";&#13;&#10;      var d="";&#13;&#10;      s=obj.options(i+1).text ;&#13;&#10;      d=obj.options(i+1).value ;&#13;&#10;      obj.options(i+1).text=obj.options(i).text ;&#13;&#10;      obj.options(i+1).value=obj.options(i).value ;&#13;&#10;      obj.options(i).text=s ;&#13;&#10;      obj.options(i).value=d ;&#13;&#10;	  obj.options(i).selected=false&#13;&#10;     obj.options(i+1).selected=true ;&#13;&#10;      break ;&#13;&#10;    }&#13;&#10;  }&#13;&#10;}&#13;&#10;//点击确定时函数&#13;&#10;function uf_ok() {&#13;&#10;	//主表&#13;&#10;	//主表表名不能为空&#13;&#10;	if(cboSelPkTab.value == "") {&#13;&#10;		alert("主表名称不能为空,请选择表名!")&#13;&#10;		return;&#13;&#10;	}&#13;&#10;	//字段不能为空&#13;&#10;	if(lstSelField2.options.length == 0){&#13;&#10;		alert("请选择字段！")&#13;&#10;		return ;&#13;&#10;	}&#13;&#10;	//主表的主键不能为空&#13;&#10;	if(cboSelPkField.value == "") {&#13;&#10;		alert("主键字段不能为空,请选择！")&#13;&#10;		return;&#13;&#10;	}&#13;&#10;	//如果子表名是否为空,为空return&#13;&#10;	if(chkAddChildTab.value == "是" ) {&#13;&#10;		if(IsSpace(cboSelChildTabName.value)) {&#13;&#10;			alert("子表名不能为空,请选择表名!")&#13;&#10;			return;&#13;&#10;		}&#13;&#10;	}&#13;&#10;	//主表名称&#13;&#10;	var PkTabName = cboSelPkTab.value ;&#13;&#10;	//求主表中文名称;&#13;&#10;	var TitleSql = "select chnname from FC_TBLIST where rtrim(tbname)='"+PkTabName+"'" ;&#13;&#10;	var sVal = SqlToField(TitleSql);&#13;&#10;	//标题&#13;&#10;	var sTitle = '<DIV onmove=move() oncontrolselect=controlselect() id=stitle onresizeend=resizeEnd() onresizestart=resizeStart() onmoveend=moveEnd() onresize=resize() style="FONT-WEIGHT: bold; FONT-SIZE: 16px; LEFT: 10px; WIDTH: 50px; COLOR: #0000ff; FONT-FAMILY: 宋体; POSITION: absolute; TOP: 35px; HEIGHT: 15px; TEXT-DECORATION: underline; backgroundColor1: #c0c0c0; " onmovestart=moveStart() noWrap controltype="label" value="Label3">'+sVal+'<