;		&#13;&#10;	}&#13;&#10;&#13;&#10;}&#13;&#10;function uf_SelTab() {&#13;&#10;	Image1.style.width = "350" ;&#13;&#10;	Image1.style.height = "110";&#13;&#10;	switch(SKDBRadioGroup2.value){&#13;&#10;		case '1':{&#13;&#10;			Image1.style.width = "315" ;&#13;&#10;			Image1.style.height = "52";&#13;&#10;			Image1.src = "../images/ef_run_FormatTab4.gif"&#13;&#10;			break;&#13;&#10;		}&#13;&#10;		case '2':{&#13;&#10;			Image1.src = "../images/ef_run_tab2.gif"&#13;&#10;			break;		&#13;&#10;		}&#13;&#10;		case '3':{&#13;&#10;			Image1.src = "../images/ef_run_tab3.gif"&#13;&#10;			break;		&#13;&#10;		}&#13;&#10;		case '4':{&#13;&#10;			Image1.src = "../images/ef_run_tab4.gif"&#13;&#10;			break;		&#13;&#10;		}&#13;&#10;		case '5':{&#13;&#10;			Image1.src = "../images/ef_run_tab5.gif"&#13;&#10;			break;		&#13;&#10;		}&#13;&#10;	}&#13;&#10;}&#13;&#10;//点击确定函数&#13;&#10;function uf_ok() {&#13;&#10;	var sHtml = "" ;&#13;&#10;	var sclassNameValue = ""&#13;&#10;	if(rdoNewTab.value == 5) {&#13;&#10;		switch(SKDBRadioGroup2.value){&#13;&#10;			case '1':{&#13;&#10;				sclassNameValue = "FormatTab" ;&#13;&#10;				break;&#13;&#10;			}&#13;&#10;			case '2':{&#13;&#10;				sclassNameValue = "TableTitle";&#13;&#10;				break;&#13;&#10;			}&#13;&#10;			case '3':{&#13;&#10;				sclassNameValue = "TableTitle1"&#13;&#10;				break;&#13;&#10;			}&#13;&#10;			case '4':{&#13;&#10;				sclassNameValue = "TableTitle2";&#13;&#10;				break;&#13;&#10;			}&#13;&#10;			case '5':{&#13;&#10;				sclassNameValue = "TableTitle1";&#13;&#10;				break;&#13;&#10;			}&#13;&#10;		}&#13;&#10;	}else{&#13;&#10;		sclassNameValue = "FormatTab" ;&#13;&#10;	}&#13;&#10;	&#13;&#10;	sHtml += "<table class='"+sclassNameValue+"' cellSpacing=0 cellPadding=0  width='75%'>" ;&#13;&#10;	switch(rdoNewTab.value){&#13;&#10;		case '1':{&#13;&#10;			sHtml += "<tr><td width='100%' height='30' style='font-size:16px;' class='FormatTabTD'></td></tr><tr><td class='FormatTabTD' width='100%' height='30' style='font-size:12px;'></td></tr>"&#13;&#10;