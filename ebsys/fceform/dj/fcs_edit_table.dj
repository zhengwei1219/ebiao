" ";&#13;&#10;			}&#13;&#10;			sSql += sNull;&#13;&#10;			if(IsSpace(sDft)==false){&#13;&#10;				sSql += " DEFAULT "+sQuot+sDft+sQuot;&#13;&#10;			}&#13;&#10;			sSql += ",";&#13;&#10;		}&#13;&#10;		var ii=0&#13;&#10;		if(fcpubdata.databaseTypeName=="oracle"){&#13;&#10;			sSql+=sFd+" "+sTxt;&#13;&#10;			if(sTxt=="NUMBER"){&#13;&#10;				ii++&#13;&#10;				sSql+=" ("+WD+","+JD+") "&#13;&#10;				if(IsSpace(sDft)==false){&#13;&#10;					sSql+="default "+sDft+" "	&#13;&#10;				}	&#13;&#10;			}&#13;&#10;			if(sTxt=="FLOAT"){&#13;&#10;				ii++&#13;&#10;				sSql+=" ("+WD+") "&#13;&#10;				if(IsSpace(sDft)==false){&#13;&#10;					sSql+="DEFAULT "+sDft+" ";	&#13;&#10;				}	&#13;&#10;			}&#13;&#10;			if(sTxt=="CHAR" || sTxt=="VARCHAR2" || sTxt=="NCHAR" || sTxt=="NVARCHAR2" || sTxt=="RAW"){&#13;&#10;				ii++ ;&#13;&#10;				sSql+=" ("+WD+") " ;&#13;&#10;				if(IsSpace(sDft)==false){&#13;&#10;					var ss=sDft;&#13;&#10;					if(ss.indexOf("'")!=-1){&#13;&#10;						ss=ss.substring(1,ss.length-1)	&#13;&#10;					}&#13;&#10;					sSql+="DEFAULT '"+ss+"' ";&#13;&#10;				}	&#13;&#10;			}&#13;&#10;			if(sTxt=="LONG RAW" || sTxt=="BLOB" || sTxt=="CLOB" || sTxt=="LONG"){&#13;&#10;				ii++&#13;&#10;				sSql+=" ";&#13;&#10;				if(IsSpace(sDft)==false){&#13;&#10;					sSql+="DEFAULT '"+sDft+" ";	&#13;&#10;				}&#13;&#10;			}&#13;&#10;			if(sTxt=="DATE"){&#13;&#10;				ii++&#13;&#10;				sSql+=" ";&#13;&#10;				if(IsSpace(sDft)==false){&#13;&#10;					sSql+="DEFAULT to_date('"+sDft+"','yyyy-mm-dd') "	&#13;&#10;				}	&#13;&#10;			}&#13;&#10;			if(ii=="0"){&#13;&#10;				sSql+=" ";&#13;&#10;			}&#13;&#10;			sSql += sNull+","&#13;&#10;		}&#13;&#10;		if(fcpubdata.databaseTypeName=="db2"){&#13;&#10;			if(sNull == "NULL") sNull="";&#13;&#10;			sSql+=sFd+" "+sTxt;&#13;&#10;			if(sTxt=="DECIMAL"){&#13;&#10;				sSql+=" ("+WD+","+JD+") ";&#13;&#10;			}&#13;&#10;			if(sTxt=="CHARACTER" || sTxt=="VARCHAR" || sTxt=="GRAPHIC" || sTxt=="VARGRAPHIC" ){&#13;&#10;				sSql+=" ("+WD+") ";&#13;&#10;			}&#13;&#10;			sSql+=" 