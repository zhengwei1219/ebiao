<script>var obj;&#13;&#10;function uf_open(){&#13;&#10;	obj=fcpubdata.obj[0];&#13;&#10;	lstPage.outerHTML = SelectAddOption(lstPage,obj.pageXml);&#13;&#10;	lstPage.selectedIndex=0;&#13;&#10;	uf_onchange();&#13;&#10;	&#13;&#10;	if(obj.isLoadAll == "是") SetCheckBoxValue(chkLoadAll,"是");&#13;&#10;	if(IsSpace(obj.iframeProps)==false) txtProps.value = obj.iframeProps;&#13;&#10;	PropWinOnload();&#13;&#10;}&#13;&#10;function uf_onchange(){&#13;&#10;//	var ilen=lstPage.options.length;&#13;&#10;//	for(var i=0;i<ilen;i++){&#13;&#10;//		if(lstPage.options(i).selected){&#13;&#10;	if(lstPage.selectedIndex < 0 ) return ;&#13;&#10;	txtTitle.value=lstPage.value;&#13;&#10;	txtUrl.value = unescape(lstPage.options(lstPage.selectedIndex).linkUrl);&#13;&#10;	if(IsSpace(lstPage.options(lstPage.selectedIndex).pageClick))&#13;&#10;		txtClick.value = ""&#13;&#10;	else&#13;&#10;		txtClick.value = unescape(lstPage.options(lstPage.selectedIndex).pageClick);&#13;&#10;		//	break;&#13;&#10;		//}&#13;&#10;	//}&#13;&#10;}&#13;&#10;function uf_edit() {&#13;&#10;	var i = lstPage.selectedIndex;&#13;&#10;	if(i < 0){&#13;&#10;		alert("请选中要修改的页!");&#13;&#10;		return;	&#13;&#10;	}&#13;&#10;	if(txtTitle.value.length>7){&#13;&#10;		alert("请缩减您的标题,最多只能七个字")&#13;&#10;		return&#13;&#10;	}&#13;&#10;	lstPage.options(i).text=txtTitle.value ;&#13;&#10;	lstPage.options(i).value=txtTitle.value ;&#13;&#10;	lstPage.options(i).linkUrl=escape(txtUrl.value) ;&#13;&#10;	lstPage.options(i).pageClick=escape(txtClick.value) ;&#13;&#10;}&#13;&#10;function uf_add() {&#13;&#10;	var stmp="<option value='页签' linkUrl='' >页签</option>";&#13;&#10;	lstPage.outerHTML = SelectAddOption(lstPage,stmp);&#13;&#10;}&#13;&#10;function uf_del() {&#13;&#10;	var k=lstPage.selectedIndex;&#13;&#10;	if(k<0){&#13;&#10;		alert("请先选择要删除的页!");&#13;&#10;		return;	&#13;&#10;	}&#13;&#10;	if(lstPage.options.length<=1){&#13;&#10;		alert("页签不能少于一个");&#13;&#10;		return;&#13;&#10;	}else{&#13;&#10;		lstPage.options.remove(k);&#13;&#10;	}&#13;&#10;	lstPage.selectedIndex=0;&#13;&#10;	uf_onchange();&#13;&#10;}&#13;&#10;&#13;&#10;function uf_ok(){&#13;&#10;	if(PropWinClickOk()) return ;&#13;&#10;	var sb = new Sys.StringBuilder()&#13;&#10;	var len = lstPage.options.length;&#13;&#10;	for(var i=0;i<len;i++){&#13;&#10;		sb.append("<option value='"+lstPage.options(i).value+"'");&#13;&#10;		sb.append(" linkUrl='"+lstPage.options(i).linkUrl+"'");&#13;&#10;		sb.append(" pageClick='"+lstPage.options(i).pageClick+"'");&#13;&#10;		sb.append(" >"+lstPage.options(i).text+"</option>");&#13;&#10;			&#13;&#10;	}&#13;&#10;	//alert(sb.toString())&#13;&#10;	obj.pageXml = sb.toString();&#13;&#10;	obj.iframeProps = txtProps.value;&#13;&#10;	obj.isLoadAll = chkLoadAll.value;&#13;&#10;	&#13;&#10;	window.close();&#13;&#10;}&#13;&#10;&#13;&#10;function uf_move(direct) {&#13;&#10;	//保存旧的&#13;&#10;	var pos = lstPage.selectedIndex ;&#13;&#10;	var nextpos = pos+1 ;&#13;&#10;	if(direct == 0) nextpos=pos-1 ; //上移&#13;&#10;	if(nextpos<0 || nextpos >= lstPage.options.length) return;&#13;&#10;	lstPage.options(pos).swapNode(lstPage.options(nextpos));	&#13;&#10;}</script> 
