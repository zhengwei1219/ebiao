<script>function uf_open() {&#13;&#10;&#13;&#10;	var obj = fcpubdata.obj;&#13;&#10;	NumEdit1.value = obj.cellPadding ;&#13;&#10;	NumEdit2.value = obj.cellSpacing;&#13;&#10;	SetButtonImage(FCButton2,"../images/ef_run_button_color.gif");&#13;&#10;	SetButtonImage(FCButton1,"../images/ef_run_button_color.gif");&#13;&#10;&#13;&#10;&#13;&#10;	if(obj.style.width == ""){&#13;&#10;		SetCheckBoxValue(SKDBcheckbox1,"否")&#13;&#10;	}else{&#13;&#10;		SetCheckBoxValue(SKDBcheckbox1,"是")&#13;&#10;		FCDBedit1.value = parseInt(obj.style.width);&#13;&#10;	}&#13;&#10;	if(obj.style.height == ""){&#13;&#10;		SetCheckBoxValue(SKDBcheckbox2,"否")&#13;&#10;	}else{&#13;&#10;		SetCheckBoxValue(SKDBcheckbox2,"是")&#13;&#10;		FCDBedit2.value = parseInt(obj.style.height);&#13;&#10;	}&#13;&#10;	//得到当前单元格的宽度&#13;&#10;	var s1 = obj.style.width;&#13;&#10;	var s2 = s1.substring(s1.length,s1.length-2);&#13;&#10;	//得到当前单元格的高度&#13;&#10;	var ss = obj.style.height ; &#13;&#10;	var ss2 = ss.substring(ss.length,ss.length-2);&#13;&#10;	if(s2 == "px"){&#13;&#10;		SetRadioValue(SKDBRadioGroup1,'1');&#13;&#10;	}else{&#13;&#10;		SetRadioValue(SKDBRadioGroup1,'2');&#13;&#10;	}&#13;&#10;	if(ss2 == "px") {&#13;&#10;		SetRadioValue(SKDBRadioGroup2,'1');	&#13;&#10;	}else{&#13;&#10;		SetRadioValue(SKDBRadioGroup2,'2');	&#13;&#10;	}&#13;&#10;	uf_ischeck()&#13;&#10;	if(IsSpace(obj.align)) {&#13;&#10;		SKDBcombobox1.value = "left" ;&#13;&#10;	}else{&#13;&#10;		SKDBcombobox1.value = obj.align;	&#13;&#10;	}&#13;&#10;	if(IsSpace(obj.border)){&#13;&#10;		NumEdit3.value = "1"&#13;&#10;	}else{&#13;&#10;		NumEdit3.value = parseInt(obj.border);&#13;&#10;	}&#13;&#10;	new Eform.SysForm().setButtonImage();&#13;&#10;}&#13;&#10;function uf_ok() {&#13;&#10;	var obj = fcpubdata.obj ;&#13;&#10;	obj.cellPadding = NumEdit1.value ;&#13;&#10;	obj.cellSpacing = NumEdit2.value ;&#13;&#10;	if(IsSpace(FCDBedit1.value) == false){&#13;&#10;		if(SKDBRadioGroup1.value == 1){&#13;&#10;			obj.style.width = FCDBedit1.value+"px";&#13;&#10;			&#13;&#10;		}else{&#13;&#10;			FCDBedit1.value = "100" ;&#13;&#10;			obj.style.width = FCDBedit1.value+"%";&#13;&#10;		}&#13;&#10;	}&#13;&#10;	if(IsSpace(FCDBedit2.value) == false){&#13;&#10;		if(SKDBRadioGroup2.value == 1) {&#13;&#10;			obj.style.height = FCDBedit2.value +"px" ;&#13;&#10;		}else{&#13;&#10;			FCDBedit2.value = "100";&#13;&#10;			obj.style.height = FCDBedit2.value + "%"&#13;&#10;		}&#13;&#10;	}&#13;&#10;	obj.align = SKDBcombobox1.value;&#13;&#10;&#13;&#10;	&#13;&#10;	obj.border = NumEdit3.value ;&#13;&#10;	window.close();&#13;&#10;}&#13;&#10;function uf_ischeck() {&#13;&#10;	if(SKDBcheckbox1.value == "否") {&#13;&#10;		FCDBedit1.disabled = true;&#13;&#10;		FCDBedit1.value = "";&#13;&#10;		SKDBRadioGroup1.disabled = true ;&#13;&#10;	}else{&#13;&#10;		FCDBedit1.disabled = false;&#13;&#10;	;&#13;&#10;		if(IsSpace(FCDBedit2.value) == true) {&#13;&#10;			//FCDBedit1.value = 100 ;&#13;&#10;			//SetRadioValue(SKDBRadioGroup1,'1') 	&#13;&#10;		}&#13;&#10;		SKDBRadioGroup1.disabled = false ;&#13;&#10;	}&#13;&#10;	if(SKDBcheckbox2.value == "否") {&#13;&#10;		FCDBedit2.disabled = true ;&#13;&#10;		FCDBedit2.value = "";&#13;&#10;		SKDBRadioGroup2.disabled = true;&#13;&#10;	}else{&#13;&#10;		FCDBedit2.disabled = false ;&#13;&#10;		SKDBRadioGroup2.disabled = false;&#13;&#10;		if(IsSpace(FCDBedit2.value) == true) {&#13;&#10;			//FCDBedit2.value = 100 ;&#13;&#10;			//SetRadioValue(SKDBRadioGroup2,'1') ;&#13;&#10;		}&#13;&#10;	}&#13;&#10;}&#13;&#10;function uf_callColorDlg(){&#13;&#10;	var obj = fcpubdata.obj ;&#13;&#10;	var sColor;&#13;&#10;	var sInitColor=obj.bgColor;&#13;&#10;	if (isSpace(sInitColor ))    	sInitColor="white"&#13;&#10;	if(typeof sInitColor=="object"){&#13;&#10;		var s=sInitColor.value;&#13;&#10;	}else{&#13;&#10;		var s=sInitColor;&#13;&#10;	}&#13;&#10;	sColor = dlgHelper.ChooseColorDlg(s);&#13;&#10;	sColor = sColor.toString(16);&#13;&#10;	if (sColor.length < 6) {&#13;&#10;		var sTempString = "000000".substring(0,6-sColor.length);&#13;&#10;		sColor = sTempString.concat(sColor);&#13;&#10;	}&#13;&#10;	obj.bgColor="#"+sColor ;&#13;&#10;}&#13;&#10;function borderColor() {&#13;&#10;	var obj = fcpubdata.obj ;&#13;&#10;	var sColor;&#13;&#10;	var sInitColor=obj.borderColor;&#13;&#10;	if (isSpace(sInitColor ))    	sInitColor="white"&#13;&#10;	if(typeof sInitColor=="object"){&#13;&#10;		var s=sInitColor.value;&#13;&#10;	}else{&#13;&#10;		var s=sInitColor;&#13;&#10;	}&#13;&#10;	sColor = dlgHelper.ChooseColorDlg(s);&#13;&#10;	sColor = sColor.toString(16);&#13;&#10;	if (sColor.length < 6) {&#13;&#10;		var sTempString = "000000".substring(0,6-sColor.length);&#13;&#10;		sColor = sTempString.concat(sColor);&#13;&#10;	}&#13;&#10;	obj.borderColor="#"+sColor ;&#13;&#10;	&#13;&#10;}</script> 
