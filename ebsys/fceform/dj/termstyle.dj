y><valid>是</valid><procvalid>否</procvalid><link>否</link><target></target><href></href><visible>是</visible><primarykey>否</primarykey><fieldvalid></fieldvalid><tag></tag></field><field><fieldname>sint</fieldname><datatype>整数</datatype><displaylabel>数字值</displaylabel><size>15</size><precision>0</precision><fieldkind>数据项</fieldkind><defaultvalue></defaultvalue><displayformat></displayformat><isnull>否</isnull><iskey>否</iskey><valid>是</valid><procvalid>否</procvalid><link>否</link><target></target><href></href><visible>是</visible><primarykey>否</primarykey><fieldvalid></fieldvalid><tag></tag></field><field><fieldname>swhere2</fieldname><datatype>字符</datatype><displaylabel>条件</displaylabel><size>2</size><precision>0</precision><fieldkind>数据项</fieldkind><defaultvalue></defaultvalue><displayformat></displayformat><isnull>否</isnull><iskey>否</iskey><valid>是</valid><procvalid>否</procvalid><link>否</link><target></target><href></href><visible>是</visible><primarykey>否</primarykey><fieldvalid></fieldvalid><tag></tag></field></fields></set>"&#13;&#10;&#13;&#10;function uf_open(){&#13;&#10;	new Eform.SysForm().setButtonImage();&#13;&#10;	SetButtonImage(FCButton13,"../images/ef_run_button_color.gif");&#13;&#10;	SetButtonImage(FCButton14,"../images/ef_run_button_font.gif");&#13;&#10;	SetRadioValue(SKDBcheckbox25,"否");&#13;&#10;	SetRadioValue(SKDBcheckbox26,"否");&#13;&#10;	SetRadioValue(SKDBcheckbox27,"是");&#13;&#10;	SetRadioValue(SKDBcheckbox29,"是");&#13;&#10;	SetRadioValue(SKDBcheckbox28,"否");&#13;&#10;	SetRadioValue(SKDBcheckbox30,"否");&#13;&#10;	SetRadioValue(SKDBcheckbox31,"否");&#13;&#10;	SetRadioValue(SKDBcheckbox32,"否");&#13;&#10;	uf_checkboxOnclick();&#13;&#10;	// displayfont.style.backgroundColor = "#ffffff"&#13;&#10;&#13;&#10;	try { //出错表示是增加&#13;&#10;		var arr = fcpubdata.obj[1]; //pubDataSet[1]&#13;&#10;		var sxml = "<root>"+arr[1]+sFieldInfo+"</root>";&#13;&#10;		if(isSpace(sxml)==false){&#13;&#10;			dssub1.OpenXmlData(sxml);&#13;&#10;			displayfont.outerHTML = arr[2] ;&#13;&#10;			FCDiv3.innerHTML=arr[3];&#13;&#10;			uf_sReturn()=arr[4];&#13;&#10;			uf_fontStyle()=arr[5];&#13;&#10;		}&#13;&#10;	}catch(e){}&#13;&#10;}&#13;&#10;function uf_checkboxOnclick(){&#13;&#10;	if(SKDBcheckbox25.value=="否"){&#13;&#10;		FCButton13.disabled=true;	&#13;&#10;	}else{&#13;&#10;		FCButton13.disabled=false;&#13;&#10;	}&#13;&#10;	if(SKDBcheckbox26.value=="否"){&#13;&#10;		FCButton14.disabled=true;	&#13;&#10;	}else{&#13;&#10;		FCButton14.disabled=false;&#13;&#10;	}&#13;&#10;	if(SKDBcheckbox28.value=="否"){&#13;&#10;	    SKDBcheckbox27.disabled=true;	&#13;&#10;	}else{&#13;&#10;		SKDBcheckbox27.disabled=false;&#13;&#10;	}&#13;&#10;	if(SKDBcheckbox30.value=="否"){&#13;&#10;	    SKDBcheckbox29.disabled=true;	&#13;&#10;	}else{&#13;&#10;		SKDBcheckbox29.disabled=false;&#13;&#10;	}&#13;&#10;	if(SKDBcheckbox32.value=="否"){&#13;&#10;	    SKDBcheckbox31.disabled=true;	&#13;&#10;	}else{&#13;&#10;		SKDBcheckbox31.disabled=false;&#13;&#10;	}&#13;&#10;}&#13;&#10;function uf_selfield() {&#13;&#10;     var arrtmp=contselfield(fcpubdata.obj[0]);&#13;&#10;     //alert(arrtmp[0])&#13;&#10;    if(typeof arrtmp != "undefined"){&#13;&#10;     	dssub1.Fields.Field['sdata'].Value=arrtmp[0];	&#13;&#10;		dssub1.Fields.Field['sfield'].Value=arrtmp[1];&#13;&#10;		dssub1.Fields.Field['schnname'].Value=arrtmp[2];&#13;&#10;		dssub1.fset_cont();&#13;&#10;     &#13;&#10;    }&#13;&#10;    //alert(FCDBedit1.value)&#13;&#10;}&#13;&#10;&#13;&#10;//条件是否合法&#13;&#10;function uf_checkCond(){&#13;&#10;	dssub1.bEdit=true;&#13;&#10;	dssub1.Update('不检查');&#13;&#10;	for(i=0;i<dssub1.RecordCount;i++){&#13;&#10;		var sDs=dssub1.oDom.documentElement.childNodes(i).childNodes(0).text;&#13;&#10;		var swh1=dssub1.oDom.documentElement.childNodes(i).childNodes(3).text;&#13;&#10;		var svl=dssub1.oDom.documentElement.childNodes(i).childNodes(4).text;&#13;&#10;		var sint=dssub1.oDom.documentElement.childNodes(i).childNodes(5).text;&#13;&#10;		var swh2=dssub1.oDom.documentElement.childNodes(i).childNodes(6).text;&#13;&#10;		//alert(IsSpace(sDs))&#13;&#10;		if(IsSpace(sDs)==false){&#13;&#10;			if((IsSpace(svl) && IsSpace(sint)) || IsSpace(swh1)){&#13;&#10;				alert("请输入该条件或比较值");&#13;&#10;				return 0;	&#13;&#10;			}&#13;&#10;		}else{&#13;&#10;			alert("请输入数据集及字段,如果要关闭窗口,请点击【取消】");&#13;&#10;			return 0;&#13;&#10;		}&#13;&#10;		&#13;&#10;		if(IsSpace(dssub1.oDom.documentElement.childNodes(i+1).childNodes(0).text)==false && dssub1.oDom.documentElement.childNodes(i+1).childNodes(0).text!="0"){&#13;&#10;			if(IsSpace(swh2)){&#13;&#10;				alert("请选择条件（或/与）")	;&#13;&#10;				return 0;&#13;&#10;			}	&#13;&#10;		}&#13;&#10;		if(swh1=="包含" || swh1=="不包含" || swh1=="开始于" || swh1=="结束于" || swh1=="为空" || swh1=="不为空"){&#13;&#10;			if(IsSpace(sint)==false){&#13;&#10;				alert("条件不合法，请重新填写");&#13;&#10;				return 0;	&#13;&#10;			}	&#13;&#10;		}&#13;&#10;		if(swh1=="为空" || swh1=="不为空"){&#13;&#10;			if(IsSpace(svl)==false){&#13;&#10;				alert("条件不合法，请重新填写");&#13;&#10;				return 0;	&#13;&#10;			}	&#13;&#10;		}&#13;&#10;		if(IsSpace(svl)==false && IsSpace(sint)==false){&#13;&#10;			alert("条件不合法，请重新填写");&#13;&#10;			return 0;	&#13;&#10;		}&#13;&#10;	}		&#13;&#10;}&#13;&#10;&#13;&#10;//条件&#13;&#10;function uf_strReturn(){&#13;&#10;	var s1="";&#13;&#10;	var sfun=uf_checkCond();&#13;&#10;	if(sfun=="0"){&#13;&#10;		return ;	&#13;&#10;	}&#13;&#10;	for(i=0;i<dssub1.RecordCount;i++){&#13;&#10;		var sDs=dssub1.oDom.documentElement.childNodes(i).childNodes(0).text;&#13;&#10;		var sfd=dssub1.oDom.documentElement.childNodes(i).childNodes(1).text;&#13;&#10;		var schn=dssub1.oDom.documentElement.childNodes(i).childNodes(2).text;&#13;&#10;		var swh1=dssub1.oDom.documentElement.childNodes(i).childNodes(3).text;&#13;&#10;		var svl=dssub1.oDom.documentElement.childNodes(i).childNodes(4).text;&#13;&#10;		var sint=dssub1.oDom.documentElement.childNodes(i).childNodes(5).text;&#13;&#10;		var swh2=dssub1.oDom.documentElement.childNodes(i).childNodes(6).text;&#13;&#10;		/*if(IsSpace(sDs)==false){&#13;&#10;			if(IsSpace(dssub1.oDom.documentElement.childNodes(i+1).childNodes(0).text)==false && dssub1.oDom.documentElement.childNodes(i+1).childNodes(0).text!="0"){&#13;&#10;				s1=s1+sDs+".Fields.Field['"+sfd+"'].Value"+uf_condCheck(swh1,svl,sint)+uf_condition2(swh2);&#13;&#10;			}else{&#13;&#10;				s1=s1+sDs+".Fields.Field['"+sfd+"'].Value"+uf_condCheck(swh1,svl,sint);&#13;&#10;			}&#13;&#10;		}*/&#13;&#10;		s1=s1+"["+schn+"]"+" "+swh1+" "+"'"+svl+"'"+sint+swh2+" ";&#13;&#10;	}&#13;&#10;	return (s1);&#13;&#10;}&#13;&#10;&#13;&#10;function uf_sReturn(){&#13;&#10;	var s1="";&#13;&#10;	var sfun=uf_checkCond();&#13;&#10;	if(sfun=="0"){&#13;&#10;		return ;	&#13;&#10;	}&#13;&#10;	for(i=0;i<dssub1.RecordCount;i++){&#13;&#10;		var sDs=dssub1.oDom.documentElement.childNodes(i).childNodes(0).text;&#13;&#10;		var sfd=dssub1.oDom.documentElement.childNodes(i).childNodes(1).text;&#13;&#10;		var schn=dssub1.oDom.documentElement.childNodes(i).childNodes(2).text;&#13;&#10;		var swh1=dssub1.oDom.documentElement.childNodes(i).childNodes(3).text;&#13;&#10;		var svl=dssub1.oDom.documentElement.childNodes(i).childNodes(4).text;&#13;&#10;		var sint=dssub1.oDom.documentElement.childNodes(i).childNodes(5).text;&#13;&#10;		var swh2=dssub1.oDom.documentElement.childNodes(i).childNodes(6).text;&#13;&#10;		if(IsSpace(sDs)==false){&#13;&#10;			if(IsSpace(dssub1.oDom.documentElement.childNodes(i+1).childNodes(0).text)==false && dssub1.oDom.documentElement.childNodes(i+1).childNodes(0).text!="0"){&#13;&#10;				s1=s1+sDs+".Fields.Field['"+sfd+"'].Value"+uf_condCheck(swh1,svl,sint)+uf_condition2(swh2);&#13;&#10;			}else{&#13;&#10;				s1=s1+sDs+".Fields.Field['"+sfd+"'].Value"+uf_condCheck(swh1,svl,sint);&#13;&#10;			}&#13;&#10;		}&#13;&#10;		//s1=s1+"["+schn+"]"+" "+swh1+" "+"'"+svl+"'"+sint+swh2+" ";&#13;&#10;	}&#13;&#10;	return (s1);	&#13;&#10;}&#13;&#10;&#13;&#10;function uf_condition2(swh2){&#13;&#10;	var s2="";&#13;&#10;	if(swh2=="或"){&#13;&#10;		s2=" || ";	&#13;&#10;	}&#13;&#10;	if(swh2=="与"){&#13;&#10;		s2=" && ";	&#13;&#10;	}&#13;&#10;	return s2;&#13;&#10;	&#13;&#10;}&#13;&#10;function uf_condCheck(swh,svl,sint){&#13;&#10;	var swhere="";&#13;&#10;	var s="'";&#13;&#10;	var ss=svl;&#13;&#10;	if(IsSpace(ss)){&#13;&#10;		ss=sint;&#13;&#10;		s="";&#13;&#10;	}&#13;&#10;	if(swh=="等于"){&#13;&#10;		swhere="=="+s+ss+s;	&#13;&#10;	}&#13;&#10;	if(swh=="不等于"){&#13;&#10;		swhere="!="+s+ss+s;	&#13;&#10;	}&#13;&#10;	if(swh=="小于"){&#13;&#10;		swhere="<"+s+ss+s;	&#13;&#10;	}&#13;&#10;	if(swh=="小于或等于"){&#13;&#10;		swhere="<="+s+ss+s;	&#13;&#10;	}&#13;&#10;	if(swh=="大于"){&#13;&#10;		swhere=">"+s+ss+s;	&#13;&#10;	}&#13;&#10;	if(swh=="大于或等于"){&#13;&#10;		swhere=">="+s+ss+s;	&#13;&#10;	}&#13;&#10;	if(swh=="包含"){&#13;&#10;		//modify by liuxr at 2009-5-26&#13;&#10;		//swhere=" like '%"+svl+"%'";&#13;&#10;		swhere=".indexOf('"+svl+"')>-1";	&#13;&#10;	}&#13;&#10;	if(swh=="不包含"){&#13;&#10;		//modify by liuxr at 2009-5-26&#13;&#10;		//swhere=" not like '%"+svl+"'";	&#13;&#10;		swhere=".indexOf('"+svl+"')<0";	&#13;&#10;	}&#13;&#10;	&#13;&#10;	//added by liuxr at 2009-5-26去掉下面的条件&#13;&#10;	/*if(swh=="开始于"){&#13;&#10;		swhere=" like '"+svl+"%'";	&#13;&#10;	}	&#13;&#10;	if(swh=="结束于"){&#13;&#10;		swhere=" like '%"+svl+"'";	&#13;&#10;	}&#13;&#10;	if(swh=="为空"){&#13;&#10;		swhere=" is null";	&#13;&#10;	}&#13;&#10;	if(swh=="不为空"){&#13;&#10;		swhere=" is not null";	&#13;&#10;	}*/&#13;&#10;	return swhere;&#13;&#10;}&#13;&#10;&#13;&#10;function uf_M(){&#13;&#10;    DjOpen('fontlist',displayfont,'展现','有模式窗口','直接','字体属性')&#13;&#10;}&#13;&#10;&#13;&#10;function uf_checkFont(){&#13;&#10;	if(SKDBcheckbox25.value=="否" && SKDBcheckbox26.value=="否" && SKDBcheckbox28.value=="否"  && SKDBcheckbox30.value=="否"  && SKDBcheckbox32.value=="否"){&#13;&#10;		alert("请输入条件格式");&#13;&#10;		return 0;&#13;&#10;	}	&#13;&#10;}&#13;&#10;function uf_fontStyle(){&#13;&#10;	var sFont="";&#13;&#10;	var curObjID;&#13;&#10;	var sfun1=uf_checkFont();&#13;&#10;	if(sfun1=="0"){&#13;&#10;		return ;	&#13;&#10;	}&#13;&#10;	if(SKDBcheckbox25.value=="是"){&#13;&#10;		sFont+="curObjID.style.backgroundColor='"+displayfont.style.backgroundColor+"';";&#13;&#10;	}&#13;&#10;	if(SKDBcheckbox26.value=="是"){&#13;&#10;		sFont+="curObjID.style.fontStyle='"+displayfont.style.fontStyle+"';";&#13;&#10;		sFont+="curObjID.style.fontFamily='"+displayfont.style.fontFamily+"';";&#13;&#10;		sFont+="curObjID.style.textDecoration='"+displayfont.style.textDecoration+"';";&#13;&#10;		sFont+="curObjID.style.fontWeight='"+displayfont.style.fontWeight+"';";&#13;&#10;		sFont+="curObjID.style.fontSize='"+displayfont.style.fontSize+"';";&#13;&#10;		sFont+="curObjID.style.color='"+displayfont.style.color+"';";&#13;&#10;	}&#13;&#10;	if(SKDBcheckbox28.value=="是"){&#13;&#10;		if(SKDBcheckbox27.value=="是"){&#13;&#10;			sFont+="curObjID.removeAttribute('disabled');";	&#13;&#10;		}else{&#13;&#10;			sFont+="curObjID.disabled='true';";&#13;&#10;		}&#13;&#10;	}&#13;&#10;	if(SKDBcheckbox30.value=="是"){&#13;&#10;		if(SKDBcheckbox29.value=="是"){&#13;&#10;			sFont+="curObjID.style.display='block';";&#13;&#10;		}else{&#13;&#10;			sFont+="curObjID.style.display='none';";&#13;&#10;		}&#13;&#10;	}&#13;&#10;	if(SKDBcheckbox32.value=="是"){&#13;&#10;		if(SKDBcheckbox31.value=="是"){&#13;&#10;			sFont+="curObjID.readOnly='true';";	&#13;&#10;		}else{&#13;&#10;			sFont+="curObjID.removeAttribute('readOnly');";&#13;&#10;		}&#13;&#10;	}&#13;&#10;	return (sFont);&#13;&#10;}&#13;&#10;function uf_ok(){&#13;&#10;	var sfun=uf_checkCond();&#13;&#10;	if(sfun=="0"){&#13;&#10;		return ;	&#13;&#10;	}&#13;&#10;	var sfun1=uf_checkFont();&#13;&#10;	if(sfun1=="0"){&#13;&#10;		return ;	&#13;&#10;	}&#13;&#10;	/*var sXml="";&#13;&#10;	sXml="<term><Node1>"+uf_strReturn()+"</Node1><Node2>"+uf_fontStyle()+"</Node2></term>";&#13;&#10;	*/&#13;&#10;	var arr=new Array();&#13;&#10;	var s1=dssub1.xml&#13;&#10;	s1 = s1.substring(6,s1.indexOf(sFieldInfo))&#13;&#10;	s1 = RepStr(s1,' rowstate="add"','')&#13;&#10;	arr[0]=uf_strReturn();&#13;&#10;	arr[1]=s1 ;&#13;&#10;	//alert(arr[1])&#13;&#10;	arr[2]=displayfont.outerHTML ;&#13;&#10;	arr[3]=FCDiv3.innerHTML;&#13;&#10;	arr[4]=uf_sReturn();&#13;&#10;	arr[5]=uf_fontStyle();&#13;&#10;	window.returnValue=arr;&#13;&#10;	window.close();&#13;&#10;		&#13;&#10;}</script> 
