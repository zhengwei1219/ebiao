        <sql>select fcq_org.sId from fcq_org where deleteMark=0 and sPersonId{{keyvalue}}</sql>
      </fcq_userTofcq_org>

      <fcq_employeeTofcq_org>
        <sql>select fcq_org.sId from fcq_org where deleteMark=0 and sPersonId{{keyvalue}}</sql>
      </fcq_employeeTofcq_org>
      <fcq_employeeTofcq_orgParent>
        <sql>select fcq_org.sParent from fcq_org where deleteMark=0 and sPersonId{{keyvalue}}</sql>
      </fcq_employeeTofcq_orgParent>
      <fcq_orgTofcq_orgAll>
